print('IF-ELSE CONDITIONAL STATEMENTS')
num = 7
if num > 5:
    print(f"Number {num} is greater than 5.")
else:
    print(f"Number {num} is 5 or less.")

print('\nWHILE LOOP')
count = 0
while count < 5:
    print(count)
    count += 1

print('\nFOR LOOP')
for i in range(5):
    print(i);

print("\nThis program is written by Niyamat)
