a=15
b=5
#BASIC ARITHMETIC OPERATORS
Sum = a+b
subtract = a-b
product = a*b
quotient = a/b
#FLOOR DIVISION AND MODULUS
floor_div = a//b
remainder = a%b
#EXPONENTATION
power = a**b
#DISPLAY RESULTS
print(f"Sum: {Sum}")
print(f"Difference: {subtract}")
print(f"Product: {product}")
print(f"Quotient: {quotient}")
print(f"Floor_div: {floor_div}")
print(f"Remainder: {remainder}")
print(f"Power: {power}")

print("\nThis program is written by Niyamat. ")
