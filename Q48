print('with Step:'.upper())
for i in range(1, 10, 2):
    print(i)

print('\nfor Iteration:'.upper())
for i in range(5):
    print(i)

print('\nwith Start and Stop:'.upper())
for i in range(2, 6):
    print(i)

print('\nwith Negative Step:'.upper())
for i in range(10, 0, -2):
    print(i)

print("\nThis program is written by Niyamat. ")
